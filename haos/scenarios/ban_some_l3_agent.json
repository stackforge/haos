{
  "NeutronL3Disaster.ban_some_l3_agents": [
    {
      "runner": {
        "type": "serial",
        "times": 1
      },
      "context": {
        "users": {
          "tenants": 1,
          "users_per_tenant": 1
        },
        "roles":[
          "admin"
        ],
        "recover_cloud": {
          "checks": ["rabbitmq_cluster_status"]
        },
        "network": {
          "networks_per_tenant": 2
        },
        "cloud": { },
        "cloud_nodes": {
          "controllers": [
            {
              "agent_endpoint": "node-5.domain.tld"
            },
            {
              "agent_endpoint": "node-4.domain.tld"
            },
            {
              "agent_endpoint": "node-3.domain.tld"
            }
          ]
        }
      }
    }
  ]
}
